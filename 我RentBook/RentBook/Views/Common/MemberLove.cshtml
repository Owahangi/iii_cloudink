        //string s = "查無資料";
        //if (reader.Read())
        //{
        //    s = reader["fName"].ToString();
        //}
        //con.Close();

        SqlConnection con = new SqlConnection();
        con.ConnectionString = @"Data Source=.;Initial Catalog=RentBookdb;Integrated Security=True";
        con.Open();

        SqlCommand cmd = new SqlCommand();
        cmd.Connection = con;
        string sql = "";
        sql = sql + "Select D.*";
        sql = sql + "  From Member A left join BookCase B on A.bc_id = B.bc_id";
        sql = sql + "  left join BookCaseBooks C on B.bc_id = C.bc_id";
        sql = sql + "  left join Books D on C.b_id = D.b_id";
        sql = sql + " Where A.m_Email = '" + user + "' ";
        cmd.CommandText = sql;

        SqlDataReader reader = cmd.ExecuteReader();
        b_name_1 = "";
        b_info_1 = "";
        b_image_1 = "";
        b_name_2 = "";
        b_info_2 = "";
        b_image_2 = "";
        b_name_3 = "";
        b_info_3 = "";
        b_image_3 = "";
        b_name_4 = "";
        b_info_4 = "";
        b_image_4 = "";
        int i = 0;

        //if (reader.Read())
        //{
        //    b_info = reader["b_info"].ToString();
        //}

        if (reader.HasRows)
        {
            while (reader.Read())
            {
                i++;

                if (i == 1)
                {
                    b_name_1 = reader["b_name"].ToString();
                    b_info_1 = reader["b_info"].ToString();
                    b_image_1 = reader["b_image"].ToString();
                }
                else if (i == 2)
                {
                    b_name_2 = reader["b_name"].ToString();
                    b_info_2 = reader["b_info"].ToString();
                    b_image_2 = reader["b_image"].ToString();
                }
                else if (i == 3)
                {
                    b_name_3 = reader["b_name"].ToString();
                    b_info_3 = reader["b_info"].ToString();
                    b_image_3 = reader["b_image"].ToString();
                }
                else if (i == 4)
                {
                    b_name_4 = reader["b_name"].ToString();
                    b_info_4 = reader["b_info"].ToString();
                    b_image_4 = reader["b_image"].ToString();
                }
            }
        }

        con.Close();



    }

    Layout = "~/Views/Shared/_CommonMemberLoveLayout.cshtml"; }

